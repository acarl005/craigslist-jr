<div class="container">
  <div class="panel panel-default">
    <div class="panel-leftheading">
        <h3 class="panel-lefttitle"><%= @article.category.name.camelize %></h3>
    </div>
    <div class="panel-rightbody">
        <h3><%= @article.title %></h3>
        <img class="img_responsive limit" src="<%= @article.img_url %>">
        <p><%= @article.body %></p>
        <span class="label label-default">Contact: <%= @article.contact %></span>
    </div>
      <div class="clearfix">
      </div>
  </div>
</div>
